    "@vitejs/plugin-react": "^2.2.0",
    "fs-extra": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4",
    "vite": "^3.2.3",
    "@pjblog/bootstrap": "^1.1.0",
    "@types/koa": "^2.13.5",
    "pjblog-theme-default": "^1.1.0",
    "mysql2": "^2.3.3"
  },
  "pjblog": {
    "type": "plugin",
